    "rsbuild-plugin-open-graph": "^1.1.2",
    "semantic-release": "^25.0.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "zephyr-rspress-plugin": "^0.1.9"
  },
  "pnpm": {
    "patchedDependencies": {
      "rsbuild-plugin-google-analytics@^1.0.4": "patches/rsbuild-plugin-google-analytics@1.0.4.patch"
    }
