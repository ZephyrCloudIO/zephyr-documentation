    "rsbuild-plugin-google-analytics": "^1.0.5",
    "rspress-plugin-file-tree": "^1.0.3",
    "rspress-plugin-mermaid": "^1.0.0",
    "rspress-plugin-reading-time": "^1.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@rspress/plugin-algolia": "^2.0.0",
    "@rspress/plugin-llms": "2.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "globals": "^17.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
