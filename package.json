    "url": "git+https://github.com/ZephyrCloudIO/zephyr-documentation.git"
  },
  "scripts": {
    "build": "rspress build",
    "dev": "rspress dev --port 3030",
    "lint": "eslint .",
    "format": "prettier --write .",
