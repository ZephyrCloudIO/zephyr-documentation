{
  "name": "zephyr-documentation",
  "version": "1.0.0",
  "description": "Zephyr Cloud documentation with module federation",
  "main": "index.js",
  "scripts": {
    "dev": "mprocs",
    "dev:host": "pnpm --filter host dev",
    "dev:ai": "pnpm --filter ai dev",
    "dev:cloud": "pnpm --filter cloud dev",
    "dev:mobile": "pnpm --filter mobile dev",
    "dev:sidepanel": "pnpm --filter sidepanel dev",
    "dev:web": "pnpm --filter web dev",
    "build": "pnpm -r build",
    "build:host": "pnpm --filter host build",
    "build:ai": "pnpm --filter ai build",
    "build:cloud": "pnpm --filter cloud build",
    "build:mobile": "pnpm --filter mobile build",
    "build:sidepanel": "pnpm --filter sidepanel build",
    "build:web": "pnpm --filter web build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "keywords": [
    "module-federation",
    "micro-frontends",
    "documentation",
    "zephyr"
  ],
  "author": "Zephyr Cloud",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "mprocs": "^0.7.3"
  }
}
