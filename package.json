    "@radix-ui/react-tooltip": "^1.2.8",
    "@rspress/core": "^2.0.0",
    "@rspress/plugin-client-redirects": "^2.0.0",
    "@rspress/plugin-preview": "^2.0.0",
    "@rspress/plugin-sitemap": "2.0.0",
    "@rspress/shared": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commitlint": "^20.3.1",
    "husky": "^9.1.7",
